<h1>Listing imd_states</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imd_states.each do |imd_state| %>
      <tr>
        <td><%= imd_state.name %></td>
        <td><%= imd_state.code %></td>
        <td><%= link_to 'Show', imd_state %></td>
        <td><%= link_to 'Edit', edit_imd_state_path(imd_state) %></td>
        <td><%= link_to 'Destroy', imd_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Imd state', new_imd_state_path %>
