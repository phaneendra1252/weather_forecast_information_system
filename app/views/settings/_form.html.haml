= form_for(@setting) do |f|
  - if @setting.errors.any?
    #error_explanation
      %h2
        = pluralize(@setting.errors.count, "error")
        prohibited this setting from being saved:
      %ul
        - @setting.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :key
    %br/
    = f.text_field :key
  .field
    = f.label :value
    %br/
    = f.text_field :value
  .actions
    = f.submit