<%= form_for @website do |f| %>
  <%= render "shared/error_messages", { object: @website } %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <%= f.fields_for :questions do |builder| %>
    <%= render "question_fields", :f => builder %>
  <% end %>
  <p><%= link_to_add_fields "Add a Question", f, :questions, "btn btn-primary", "Add a new Contact" %></p>
  <p><%= f.submit "Submit" %></p>
<% end %>
